<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Font">
        <attr name="font_font" format="string" />
    </declare-styleable>

    <declare-styleable name="AspectRatioImageView">
        <attr name="aspectRatio" format="float" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>

    <style name="btnStyleAccent" parent="Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:textColor">#ffffff</item>
        <item name="android:padding">15dip</item>
    </style>

</resources>
